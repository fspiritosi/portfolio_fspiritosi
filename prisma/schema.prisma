generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mysql"
  url = env("DATABASE_URL")
  relationMode = "prisma"
}

model Guestbook {
  id String @id @default(uuid())
  message String
  username String
  created_at DateTime @default(now())
}

model Message {
  id String @id @default(uuid())
  name String
  email String
  subjet String
  content String
  created_at DateTime @default(now())
}